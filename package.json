{
  "name": "inspire-api",
  "version": "0.1.0",
  "description": "An API for the INSPIRE Index Polygons dataset. Look up an Inspire id for a given lng lat",
  "scripts": {
    "start": "./server.js",
    "watch": "nodemon ./server.js --exec babel-node",
    "test": "standard",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tableflip/inspire-api.git"
  },
  "author": "TABLEFLIP",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/tableflip/inspire-api/issues"
  },
  "homepage": "https://inspire.tableflip.io",
  "dependencies": {
    "apollo-server": "^0.2.8",
    "body-parser": "^1.15.2",
    "boom": "^4.2.0",
    "express": "4.13.4",
    "graphql": "^0.7.2",
    "graphql-tools": "^0.7.2",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "request-promise": "^4.1.1",
    "sequelize": "^3.27.0"
  },
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-polyfill": "6.5.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-import": "^1.1.0",
    "eslint-plugin-react": "^4.2.3",
    "graphql": "^0.5.0",
    "nodemon": "^1.9.1",
    "standard": "^8.6.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    },
    "env": {
      "mocha": true
    }
  }
}
